<header class="l-header">
    <div class="l-header__inner">

        <div class="l-header__first">
            <h1 class="l-header__logo">
                <a href="" class="l-header__link"><img class="l-header__logo-img" src="assets/img/logo/logo.png" alt="logo"></a>
            </h1>
        </div>

        <div class="l-header__last">
            <div class="l-header__toggle-btn"><span></span><span></span></div>
        </div>

        <!-- オーバーレイ メニュー -->
        <nav class="l-header__nav">
            <ul class="l-header__menu">
                <% headerMenu.forEach(item => { %>
                <li class="l-header__menu-item"><a href="#"><%= item %></a></li>
                <% }); %>
            </ul>
        </nav>

        <div class="l-header__overlay"></div>

    </div>
</header>